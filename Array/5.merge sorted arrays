class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int i1 =0;
        int i2 =0;
        int count =0;
        if(nums2.length ==0){
            return ;
        }
        if(nums1.length==0){
            return;
        }
        for(int i=0;i<nums2.length;i++){
            nums1[m+i]=nums2[i];
        }
    Arrays.sort(nums1);
    }
}
