class Solution {
    public void moveZeroes(int[] array) {
        int count=0;
        for(int i =0;i<array.length;i++){
            if(array[i]!=0){
                array[count]=array[i];
                count++;
            }
            
        }while(count<array.length){
            array[count]=0;
            count++;
        }
        
    }
}
